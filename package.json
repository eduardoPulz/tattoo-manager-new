{
  "name": "tattoo-manager",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/setup-db.js && next dev",
    "prebuild": "echo 'Executando prebuild personalizado...' && node scripts/generate-env.js && node scripts/setup-db.js",
